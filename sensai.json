{
	"sensai_version": {
		"name": "SensAI Version",
		"type": "dropdown",
		"options": {
			"v3.0": "latticesemi/sensai:v3.0",
			"v3.1": "latticesemi/sensai:v3.1"
		},
		"default": "v3.0",
		"required": true
	},
	"sensai_options": {
		"command": {
			"run_type": "/lsc_ml_sdk/linux/lsc_ml_compl"
		},
		"cmd": {
			"name": "Command",
			"type": "checkboxes",
			"options": {
				"Analyze": "analyze",
				"Compile": "compile",
				"Simulate": "simulate"
			},
			"mandatory": "Analyze",
			"default": "Analyze, Compile, Simulate",
			"required": true
		},
		"project_name": {
			"name": "SensAI Project Name",
			"type": "str",
			"required": true
		},
		"nnMode": {
			"name": "Class",
			"type": "dropdown",
			"options": {
				"CNN": 0,
				"BNN": 1
			},
			"default": "CNN"
		},
		"framework": {
			"name": "Framework",
			"type": "dropdown",
			"options": {
				"TensorFlow": "TensorFlow",
				"Keras": "Keras",
				"Caffe": "Caffe"
			},
			"default": "TensorFlow",
			"TensorFlow": {
				"network_file": {
					"name": "TensorFlow PB file",
					"type": "file",
					"required": "true",
					"extenstions": "pb"
				},
				"image_files": {
					"name": "Input image",
					"type": "file_upload",
					"required": "true",
					"extenstions": "jpeg, jpg, png"
				}
			},
			"Caffe": {
				"model_file": {
					"name": "Caffe Model file",
					"type": "file",
					"required": "true",
					"extenstions": "caffemodel"
				},
				"network_file": {
					"name": "Caffe proto file",
					"type": "file",
					"required": "true",
					"extenstions": "proto, prototxt"
				},
				"image_files": {
					"name": "Input image",
					"type": "file_upload",
					"required": "true",
					"extenstions": "jpeg, jpg, png"
				}
			},
			"Keras": {
				"network_file": {
					"name": "Keras Model file",
					"type": "file",
					"required": "true",
					"extenstions": "h5"
				},
				"input_file": {
					"name": "Input image",
					"type": "file_upload",
					"required": "true",
					"extenstions": "jpeg, jpg, png"
				}
			}
		},
		"device": {
			"name": "Device",
			"type": "dropdown",
			"options": {
				"ECP5": "ECP5",
				"Ultra Plus": "Ultra Plus",
				"CrossLink-NX": "CrossLink-NX"
			},
			"default": "CrossLink-NX",
			"ECP5": {
				"mobilenet_mode": {
					"name": "MobileNet Mode",
					"type": "checkbox",
					"options": {
						"unchecked": 0,
						"checked": 1
					},
					"default": "unchecked"
				},
				"collapse_layer": {
					"name": "Collpase",
					"type": "checkbox",
					"options": {
						"unchecked": 0,
						"checked": 1
					},
					"default": "checked"
				},
				"scale": {
					"name": "Scale",
					"type": "float",
					"default": 1.0
				},
				"mean": {
					"name": "Mean",
					"type": "float",
					"default": 0
				}
			},
			"Ultra Plus": {
				"scale": {
					"name": "Scale",
					"type": "float",
					"default": 1.0
				},
				"mean": {
					"name": "Mean",
					"type": "float",
					"default": 0
				},
				"required_output_depth_range": {
					"name": "Required output depth range",
					"type": "str",
					"default": ""
				}
			},
			"CrossLink-NX": {
				"scale": {
					"name": "Scale",
					"type": "float",
					"default": 0.0078125
				},
				"mean": {
					"name": "Mean",
					"type": "float",
					"default": 0
				},
				"required_output_depth_range": {
					"name": "Required output depth range",
					"type": "str",
					"default": ""
				},
				"crosslink_data_base_addr": {
					"name": "Data Section Base address",
					"type": "int",
					"default": 4194304
				},
				"crosslink_code_base_addr": {
					"name": "Code Section Base address",
					"type": "int",
					"default": 0
				},
				"enable_dualcore": {
					"name": "Dual Core Mode",
					"type": "checkbox",
					"options": {
						"unchecked": 0,
						"checked": 1
					},
					"default": "unchecked"
				},
				"hyper_ram": {
					"name": "HyperRAM",
					"type": "checkbox",
					"options": {
						"unchecked": 0,
						"checked": 1
					},
					"default": "checked"
				},
				"load_from_extmem": {
					"name": "Store Input",
					"type": "checkbox",
					"options": {
						"unchecked": 0,
						"checked": 1
					},
					"default": "checked"
				},
				"store_to_extmem": {
					"name": "Store Output",
					"type": "checkbox",
					"options": {
						"unchecked": 0,
						"checked": 1
					},
					"default": "unchecked"
				}
			}
		}
	}
}
