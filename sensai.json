{
    "sensai_version":{
        "name":"SensAI Version",
        "type":"dropdown",
        "options":{
            "v3.0":"latticesemi/sensai:v3.0",
            "v3.1":"latticesemi/sensai:v3.1",
            "v4.0":"latticesemi/sensai:v4.0",
            "v4.1":"latticesemi/sensai:v4.1",
            "v5.0":"latticesemi/sensai:v5.0"
        },
        "default":"v5.0",
        "required":true
    },
    "sensai_options":{
        "command":{
            "run_type":"/lsc_ml_sdk/linux/lsc_ml_compl"
        },
        "cmd":{
            "name":"Command",
            "type":"checkboxes",
            "options":{
                "Analyze":"analyze",
                "Compile":"compile",
                "Simulate":"simulate"
            },
            "mandatory":"Analyze",
            "default":"Analyze, Compile, Simulate",
            "required":true
        },
        "project_name":{
            "name":"SensAI Project Name",
            "type":"str",
            "required":true
        },
        "nnMode":{
            "name":"Class",
            "type":"dropdown",
            "options":{
                "CNN":0,
                "BNN":1
            },
            "default":"CNN" 
        },
        "framework":{
            "name":"Framework",
            "type":"dropdown",
            "options":{
                "TensorFlow":"TensorFlow",
                "Keras":"Keras",
                "Caffe":"Caffe"
            },
            "default":"TensorFlow",
            "TensorFlow":{
                "network_file":{
                    "name":"TensorFlow PB file",
                    "type":"file",
                    "required":"true",
                    "extenstions":"pb"
                },
                "image_files":{
                    "name":"Input image",
                    "type":"file_upload",
                    "required":"true",
                    "extenstions":"jpeg, jpg, png"
                }
            },
            "Caffe":{
                "model_file":{
                    "name":"Caffe Model file",
                    "type":"file",
                    "required":"true",
                    "extenstions":"caffemodel"
                },
                "network_file":{
                    "name":"Caffe proto file",
                    "type":"file",
                    "required":"true",
                    "extenstions":"proto, prototxt"
                },
                "image_files":{
                    "name":"Input image",
                    "type":"file_upload",
                    "required":"true",
                    "extenstions":"jpeg, jpg, png"
                }
            },
            "Keras":{
                "network_file":{
                    "name":"Keras Model file",
                    "type":"file",
                    "required":"true",
                    "extenstions":"h5"
                },
                "image_files":{
                    "name":"Input image",
                    "type":"file_upload",
                    "required":"true",
                    "extenstions":"jpeg, jpg, png"
                }
            }
        },
        "device":{
            "name":"Device",
            "type":"dropdown",
            "options":{
                "ECP5":"ECP5",
                "Crosslink NX Compact":"CrossLink-NX",
                "Ultra Plus":"Ultra Plus",
                "CrossLink NX Optimized":"CrossLink-NX",
                "Certus-Pro NX Optimized":"Certus-Pro NX",
                "Certus-Pro NX Extended":"Certus-Pro NX",
                "Certus-Pro NX Compact":"Certus-Pro NX"
            },
            "default":"Certus-Pro NX Extended",
            "Crosslink NX Compact":{
                "ip":{
                    "name":"ip_mode",
                    "type":"str",
                    "default":"Compact_CNN",
                    "version":["v5.0"]
                    },
                "conv1x1_mode":{
                        "name":"conv1x1 mode",
                        "type":"dropdown",
                        "options":{
                            "Single":"single",
                            "Dual":"dual",
                            "Quad":"quad"
                        },
                        "default":"Single",
                        "version":["v3.1","v4.0","v4.1","v5.0"]
                    },
                    "crosslink_scratch_pad_blk_size":{
                        "name":"Scratch Pad Memory Block Size",
                        "type":"dropdown",
                        "options":{
                            "1024":"1024",
                            "2048":"2048",
                            "4096":"4096",
                            "8192":"8192"
                        },
                        "default":"8192",
                        "version":["v3.1","v4.0","v4.1","v5.0"]
                    },
                    "enable_dualcore":{
                        "name":"Dual Core Mode",
