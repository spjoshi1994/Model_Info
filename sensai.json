                            "65536":"65536",
                            "131072":"131072",
                            "262144":"262144"
                        },
                        "default":"131072",
                        "version":["v3.0","v3.1","v4.0","v4.1","v5.0"]
                    },
                    "reg_out":{
                        "name":"Register Out",
                        "type":"checkbox",
